{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": [ "src/js/openkarotz.js" ],
    	"exclude": [ "./dist", "./doc", "./test" ],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"template": "templates/default",
		"encoding": "utf8",
		"destination": "./doc/api/",
		"private": false,
		"recurse": false,
		"lenient": false
	}
}